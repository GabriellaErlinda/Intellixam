{
    "functions": {
      "backend/**/*.py": { 
        "runtime": "python3.11" 
      }
    },
    "rewrites": [
      {
        "source": "/backend/(.*)",
        "destination": "/backend/app" 
      },
      {
        "source": "/((?!api/|backend/|_next/|static/|favicon.ico|logo).*)",
        "destination": "/index.html"
      }
    ]
  }